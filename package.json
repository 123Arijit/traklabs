{
  "name": "traklabs",
  "version": "1.0.0",
  "description": "TrakLabs Assignment by Arijit",
  "main": "index.ts",
  "scripts": {
    "tsc": "tsc",
    "start": "node dist",
    "build": "npm run tsc",
    "dev": "ts-node-dev --respawn ./src/index.ts",
    "test": "ava-ts --concurrency 1 **/*.spec.ts",
    "test:model": "ava-ts --concurrency 1 src/model/**/*.spec.ts",
    "test:controller": "ava-ts --concurrency 1 src/controller/**/*.spec.ts",
    "test:router": "ava-ts --concurrency 1 src/router/**/*.spec.ts",
    "test:service": "ava-ts --concurrency 1 src/service/**/*.spec.ts",
    "postinstall": "npm run tsc"
  },
  "author": "Arijit Debbarma",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/lodash": "^4.14.166",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.16",
    "ava": "^3.14.0",
    "ava-ts": "^0.25.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.11.9",
    "rabbit-queue": "^5.0.1"
  }
}
